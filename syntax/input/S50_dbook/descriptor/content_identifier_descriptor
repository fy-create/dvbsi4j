tag=0x76
content_identifier_descriptor(){
    descriptor_tag               8       uimsbf  
    descriptor_length            8       uimsbf  
    for (i=0;i<N;i++) {
        crid_type                6       uimsbf  
        crid_location            2       uimsbf  
        If (crid_location == 0 ) {
            crid_length          8       uimsbf  
            for (j=0;j<crid_length;j++) {
                crid_byte        8       uimsbf  
            }
        }
        if (crid_location == 1 ) {
            crid_ref             16      uimsbf  
        }
    }
}
