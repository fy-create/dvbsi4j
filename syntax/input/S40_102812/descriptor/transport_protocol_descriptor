tag=0x2
transport_protocol_descriptor() {
    descriptor_tag                  8       uimsbf  
    descriptor_length               8       uimsbf  
    protocol_id                     16      uimsbf  
    transport_protocol_label        8       uimsbf  
    for(i=0; i<N; i++) {
        selector_byte               8       uimsbf  
    }
}
