tag=0x02
CP_descriptor() {
    descriptor_tag                  8       uimsbf  
    descriptor_length               8       uimsbf  
    descriptor_tag_extension        8       uimsbf  
    CP_system_id                    16      uimsbf  
    reserved                        3       bslbf   
    CP_PID                          13      uimsbf  
    for (i=0; i<N; i++) {
        private_data_byte           8       uimsbf  
    }
}
