tag=0x9
CA_descriptor() {
    descriptor_tag               8       uimsbf  
    descriptor_length            8       uimsbf  
    CA_system_ID                 16      uimsbf  
    reserved                     3       bslbf   
    CA_PID                       13      uimsbf  
    for ( i=0; i<N; i++) {
        private_data_byte        8       uimsbf  
    }
}
